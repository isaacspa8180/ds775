dvar float+ x[1..4];
dvar boolean y[1..4];
dvar boolean z;
float M = 1000000;

maximize 70*x[1] + 60*x[2] + 90*x[3] + 80*x[4] 
			- 50000*y[1] - 40000*y[2] - 70000*y[3] - 60000*y[4];

subject to{
	ct1:
		x[1] <= M*y[1];

	ct2:
		x[2] <= M*y[2];
		
	ct3:
		x[3] <= M*y[3];

	ct4:
		x[4] <= M*y[4];
		
	ct5:
		y[1] + y[2] + y[3] + y[4] <= 2;

//	ct6:
//		x[3] <= x[1] + x[2];		
//
//	ct7:
//		x[4] <= x[1] + x[2];	
		
	ct6:
		y[3] <= y[1] + y[2];		

	ct7:
		y[4] <= y[1] + y[2];					
			
	ct8:
		5*x[1] + 3*x[2] + 6*x[3] + 4*x[4] <= 6000 + M*z;

	ct9: 
		4*x[1] + 6*x[2] + 3*x[3] + 5*x[4] <= 6000 + M*(1-z);
		
	ct10:
		x[1] >= 0;	
	ct11:
		x[2] >= 0;	
	ct12:
		x[3] >= 0;
	ct13:
		x[4] >= 0;			
};

 