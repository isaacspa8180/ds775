 using CP;
 
 int n = 7;
 int M = 1000;
 int c[1..n][1..n] = 
 [ [ 0, 12, 10,  M,  M,  M, 12],
   [12,  0,  8, 12,  M,  M,  M],
   [10,  8,  0, 11,  3,  M,  9],
   [ M, 12, 11,  0, 11, 10,  M],
   [ M,  M,  3, 11,  0,  6,  7],
   [ M,  M,  M, 10,  6,  0,  9],
   [12,  M,  9,  M,  7,  9,  0]];
   
  range R = 2..n;
  dvar int x[R] in R;
  
  minimize 
  
  sum (j in R) c[x[j]][x[j+1]];

  
  subject to {
  
    
  }
  
  
  
  
 